#merge two sorted list

list1=[]
list2=[]
a=int(input("Enter the number of elements in first list : "))
for i in range(1,a+1):
    b=int(input("Enter element : "))
    list1.append(b)
c=int(input("Enter no of elements in second list: "))
for i in range(1,b+1):
    d=int(input("Enter a element : "))
    list2.append(d)

list3=list1+list2
list3.sort()
print("Sorted and merged list is : ",list3)

    
      
    
